    <packagedElement xmi:type="uml:UseCase" xmi:id="_oyAvUBUpEeu0_vsCmRFoAg" name="Register car">
      <ownedComment xmi:type="uml:Comment" xmi:id="_BVr1EBd9Eeu_HeiENBoE8g">
        <body>The driver registers the car he will use to transport users, when he uses the app for the first time.</body>
      </ownedComment>
      <include xmi:type="uml:Include" xmi:id="_Eub98BebEeuZ7J-Q7sD33g" addition="_CR7a4Bd9Eeu_HeiENBoE8g"/>
    </packagedElement>
    <packagedElement xmi:type="uml:UseCase" xmi:id="_shHPYBd7Eeu_HeiENBoE8g" name="Check If credentials match">
      <ownedComment xmi:type="uml:Comment" xmi:id="_doNsABd8Eeu_HeiENBoE8g">
        <body>This UC checks if the credentials used to login match an account in the system.</body>
      </ownedComment>
    </packagedElement>
    <packagedElement xmi:type="uml:UseCase" xmi:id="_20rw8Bd7Eeu_HeiENBoE8g" name="Check Payment Method">
      <ownedComment xmi:type="uml:Comment" xmi:id="_rOdfQBd8Eeu_HeiENBoE8g">
        <body>This UC checks if the payment method used by the user is valid and if it has enough credit.</body>
      </ownedComment>
    </packagedElement>
    <packagedElement xmi:type="uml:UseCase" xmi:id="_CR7a4Bd9Eeu_HeiENBoE8g" name="Check If Car Macthes The Conditions">
      <ownedComment xmi:type="uml:Comment" xmi:id="_QLt1MBd9Eeu_HeiENBoE8g">
        <body>This UC checks if the car that the drivers wants to use, matches a certaain amount of conditions.</body>
      </ownedComment>
    </packagedElement>
